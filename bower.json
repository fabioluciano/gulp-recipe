{
    "name" : "meuclip",
    "version" : "0.0.0",
    "description" : "Application mockup tests",
    "keywords" : [
        "meuclip"
    ],
    "authors" : [
        "FÃ¡bio Luciano"
    ],
    "license" : "MIT",
    "homepage" : "http://meuclip.com",
    "private" : true,
    "dependencies": {
        "angular" : "latest",
        "angular-material" : "latest",
        "font-awesome" : "latest"
    },
    "install" : {
        "path" : {
            "css"     : "src/assets/library/{name}/stylesheet",
            "js"      : "src/assets/library/{name}/javascript",
            "eot"     : "src/assets/library/{name}/fonts",
            "otf"     : "src/assets/library/{name}/fonts",
            "svg"     : "src/assets/library/{name}/fonts",
            "ttf"     : "src/assets/library/{name}/fonts",
            "woff"    : "src/assets/library/{name}/fonts",
            "woff2"   : "src/assets/library/{name}/fonts"

        },
        "sources" : {
            "angular" : [
                "bower_components/angular/angular.min.js"
            ],
            "angular-animate" : [
                "bower_components/angular-animate/angular-animate.min.js"
            ],
            "angular-aria" : [
                "bower_components/angular-aria/angular-aria.min.js"
            ],
            "hammerjs" : [
                "bower_components/hammerjs/hammer.min.js"
            ],
            "font-awesome" : [
                "bower_components/font-awesome/css/font-awesome.min.css",
                "bower_components/font-awesome/fonts/FontAwesome.otf",
                "bower_components/font-awesome/fonts/fontawesome-webfont.eot",
                "bower_components/font-awesome/fonts/fontawesome-webfont.svg",
                "bower_components/font-awesome/fonts/fontawesome-webfont.ttf",
                "bower_components/font-awesome/fonts/fontawesome-webfont.woff",
                "bower_components/font-awesome/fonts/fontawesome-webfont.woff2"
            ],
            "angular-material" : [
                "bower_components/angular-material/angular-material.min.js",
                "bower_components/angular-material/angular-material.min.css",
            ]
        }
    }
}
